<?xml version="1.0" encoding="UTF-8"?>
<messages>
	<!--数据结构类配置规则-->
	<data name="ATKData" namespace="TheStory" type="Outside">
		<field type="int" name="id"/>
		<field type="float" name="pre"/>
		<field type="float" name="value"/>
		<field type="float" name="valueAdd"/>
		<field type="float" name="valuePre"/>
		<field type="enum" data="AtkType" name="atkType"/>
		<field type="bool" name="canAtk"/>
	</data>
	<data name="BuffDetile" namespace="TheStory" type="Outside">
		<field type="int" name="buffId"/>
		<field type="int" name="buffValue"/>
	</data>
	<data name="RoomUserData" namespace="TheStory" type="Outside">
		<field type="int" name="connectId"/>
		<field type="int" name="characterId"/>
		<field type="string" name="name"/>
		<field type="bool" name="ifSure"/>
		<field type="list" T="BuffDetile" name="skills"/>
		<field type="list" T="int" name="tags"/>
	</data>
	<data name="PlayerData" namespace="TheStory" type="Inside">
		<field type="int" name="id"/>
		<field type="float" name="atk"/>
		<field type="bool" name="sex"/>
		<field type="long" name="lev"/>
		<field type="array" data="int" name="arrays"/>
		<field type="list" T="int" name="list"/>
		<field type="dic" Tkey="int" Tvalue="string" name="dic"/>
	</data>
	<data name="AccountData" namespace="TheStory" type="Inside">
		<field type="string" name="account"/>
		<field type="string" name="password"/>
	</data>
	<!--消息类类配置规则-->
	<message name="C2S_JionRoom" namespace="TheStory" type="Server">
		<field type="string" name="name"/>
	</message>
</messages>